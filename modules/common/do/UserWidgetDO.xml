<BizDataObj Name="UserWidgetDO" Description="" Class="BizDataObj" DBName="Default" Table="user_widget" SearchRule="" SortRule="[column], [ordering] ASC" OtherSQLRule="" Uniqueness="" Stateless="N" IdGeneration="Identity" CacheLifeTime="0">
